{
  "name": "new_abedin_app",
  "displayName": "new_abedin_app",
  "expo": {
    "name": "new_abedin_app",
    "slug": "newabedinapp",
    "version": "1.0.0",
    "ios": {
      "bundleIdentifier": "com.new_abedin_app"
    },
    "runtimeVersion": "1",
    "updates": {
      "url": "http://localhost:3000/api/manifest",
      "enabled": true,
      "fallbackToCacheTimeout": 30000,
      "codeSigningCertificate": "./code-signing/certificate.pem",
      "codeSigningMetadata": {
        "keyid": "main",
        "alg": "rsa-v1_5-sha256"
      }
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.new_abedin_app"
    }
  }
}
